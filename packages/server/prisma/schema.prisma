generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("PRISMA_DATABASE_URL")
}

model Proxy {
  ip       String
  port     Int
  username String
  password String
  active   Boolean @default(true)
  vendor   String

  @@id([ip, port])
}

model ProxyIpReservation {
  ip         String @id
  serviceId  String
  instanceId String
}
